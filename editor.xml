    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledLanguages">
      <set>
        <option value="JAVA" />
        <option value="Scala" />
        <option value="go" />
        <option value="kotlin" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseOneLineMethods" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern pattern="\b(todo|fixme):\s+\[(g|G)(k|K)(f|F)\]" />
    <filter name="GkF">
      <pattern index="1" />
    </filter>
  </component>
